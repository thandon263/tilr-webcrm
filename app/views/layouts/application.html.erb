<!DOCTYPE html>
<html>
  <head>
    <title>Crm</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <!-- BOOTSTRAP STYLES-->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <!-- FONTAWESOME ICONS STYLES-->
    <link rel="stylesheet" href="https://path/to/font-awesome/css/font-awesome.min.css">    <!--CUSTOM STYLES-->
    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/css/materialize.min.css">
    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/css/materialize.min.css">
    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/js/materialize.min.js"></script>

  </head>

  <body>
    <!-- navigation tag partial rendered -->
    <%= render partial: 'layouts/navigation' %>

    <!-- search bar partial hidden -->

    <!-- Side bar nav -->
    <%= render partial: 'layouts/sideBar' %>
    <!-- Yield a block of code -->
    <%= yield %>

    <%= render partial: 'layouts/onlineChat' %>

    <%= render partial: 'layouts/importFiles' %>

    <!-- footer of the page -->
    <%#= render partial: 'layouts/footer' %>
  </body>
</html>
